    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Fasodev\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fasodev\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "tests": "vendor/bin/phpunit",
        "testsVerbose": "@tests -vvv"
    }
}
